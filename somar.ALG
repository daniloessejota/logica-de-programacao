algoritmo "Conta de Somar"
var
   N1, N2, N3, S: Inteiro
   
inicio
     Escreva ("Digite um numero qualquer ")
     Leia (N1)
     Escreva ("Digite um numero qualquer ")
     Leia (N2)
     Escreva ("Digite um numero qualquer ")
     Leia (N3)
     S <- N1+N2+N3
     Escreva ("A Soma total de ", N1, " e ", N2, " e ", N3,  " é igual a ", S)
fimalgoritmo