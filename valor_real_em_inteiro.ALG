algoritmo "FUNÇÕES - VALOR REAL EM VALOR INTEIRO"
var
   DIVISAO, VALOREAL: REAL
   VALORINTEIRO: INTEIRO

inicio
  DIVISAO <- 1000/23
  VALOREAL <- DIVISAO
  VALORINTEIRO <- int (DIVISAO)
  EscrevaL ("A divisão de 1000 por 23 em valores INTEIROS é: ", VALORINTEIRO)
  
  EscrevaL ("Fim do Programa")

fimalgoritmo