algoritmo "Descobrindo o Seu Ano de Nascimento"
var
   a, an, id: inteiro
   n: caractere

inicio
      Escreval ("Olá!")
      Escreva ("Qual é seu nome? ")
      Leia (n)
      
      Escreva (n, " em que ano estamos? ")
      Leia (a)

      Escreva ("Qual a sua idade? ")
      Leia (id)

      an <- a-id
      Escreva (N, " , você nasceu em: ", an)
      
fimalgoritmo