algoritmo "Contador Númerico"
var
   contador, valor, soma: real
inicio
      soma <- 0
      contador <- 1
      Enquanto (contador <=5) faca
      Escreva ("Digite o, ", contador, "º valor: ")
      Leia (valor)
      contador <- contador + 1
      soma <- valor + soma
      FimEnquanto
      Escreva ("Esta é a soma dos números: ", soma)
fimalgoritmo